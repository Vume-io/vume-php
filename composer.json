{
    "name": "vume/vume-php",
    "description": "PHP library for the headless Vume CMS",
    "keywords": [
        "cms",
        "api",
        "sdk"
    ],
    "authors": [
        {
            "name": "Maarten Peeters",
            "email": "maarten@roxorstudios.com",
            "role": "Developer"
        },
        {
            "name": "Glenn Engelen",
            "email": "glenn@roxorstudios.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/var-dumper": "^5.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Vume\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vume\\Tests\\": "tests/"
        }
    }
}